{
  "name": "@switchboard-xyz/v2-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "echo \"Use 'npm run build:site' to build the docusaurus project\" && exit 0",
    "build:site": "docusaurus build --out-dir public",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus build --out-dir public && docusaurus deploy --out-dir public",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve --dir public",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "update:tasks": "protoc --proto_path=../tasks/v2-task-library/protos --doc_out=api --doc_opt=markdown,_tasks.md ../tasks/v2-task-library/protos/job_schemas.proto",
    "api:cli": "shx ./scripts/cli.sh",
    "spellcheck": "cspell --config ./cSpell.json \"**/*.md*\"",
    "typecheck": "tsc",
    "test": "echo \"No test script for @switchboard-xyz/v2-docs\" && exit 0"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-beta.18",
    "@docusaurus/preset-classic": "^2.0.0-beta.18",
    "@docusaurus/remark-plugin-npm2yarn": "^2.0.0-beta.18",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@fontsource/roboto": "^4.5.1",
    "@mdx-js/react": "^1.6.21",
    "@mui/icons-material": "^5.1.1",
    "@mui/material": "^5.2.7",
    "@solana/web3.js": "^1.35.1",
    "@svgr/webpack": "^5.5.0",
    "@switchboard-xyz/sdl": "^0.0.75",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "my-loaders": "file:plugins/my-loaders",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.3.1",
    "react-player": "^2.9.0",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-beta.18",
    "@docusaurus/types": "^2.0.0-beta.18",
    "@tsconfig/docusaurus": "^1.0.4",
    "babel-plugin-import": "^1.13.3",
    "cspell": "^5.15.2",
    "shx": "^0.3.4",
    "typescript": "^4.3.5"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "crypto": false
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
